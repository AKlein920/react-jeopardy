{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "warn"
        ],
        "import/no-extraneous-dependencies": 0,
        "react/jsx-filename-extension": 0,
        "no-console": 0
    }
}